<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tiantian.mapper.TbUserMapper">



    <select id="getUserById" resultType="com.tiantian.entity.TbUser">
        SELECT tb_user.user_id,tb_user.user_name,tb_user.real_name,tb_user.PWD,tb_user.salt,tb_user.STATUS FROM tb_user WHERE user_id = #{user_id}
    </select>
</mapper>
